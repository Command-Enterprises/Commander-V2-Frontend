---
import "../styles/index.css";
import Navbar from "../components/navbar.astro";
import Boilerplate from "../components/boilerplate.astro";
---
<html>
	<head>
		<Boilerplate />
		<link rel="icon" type="image/x-icon" href="https://t3.gstatic.com/faviconV2?client=SOCIAL&type=FAVICON&fallback_opts=TYPE,SIZE,URL&url=https://docs.google.com/document/u/0/&size=16">
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap" rel="stylesheet">
		<link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;1,100;1,200;1,300;1,400;1,500&display=swap" rel="stylesheet">
	</head>
	<body>
		<div>
			<h1 class="titleh1">Commander <p class="titlep">V2</p></h1>
			<div id="navbar">
				<Navbar />
			</div>
			<div>
				<p id="bodytext">Bypass internet censorship</p>
				<ion-icon name="search-outline" id="searchIcon"></ion-icon>
				<input type="text" id="url"></input>
			</div>
			<script type="module" src="https://cdn.jsdelivr.net/npm/ionicons@7.1.0/dist/ionicons/ionicons.esm.js"></script>
			<script nomodule src="https://cdn.jsdelivr.net/npm/ionicons@7.1.0/dist/ionicons/ionicons.js"></script>
		</div>
	</body>
	
</html>